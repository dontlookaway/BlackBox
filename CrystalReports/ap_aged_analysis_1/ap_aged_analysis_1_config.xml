}
//Function for cboCurrencyValues_SelectedIndexChanged
public void cboCurrencyValues_SelectedIndexChanged ()
{
	if(cboCurrencyValues.SelectedIndex ==1)
	{
		grpCurrency.Enabled = true;
		cboCurrency.SelectedIndex = 1;
		cboCurrency.Enabled = false;
		cboConvertToLocalValue.Enabled = false;
